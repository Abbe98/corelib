<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
	">

	<bean id="corelib_db_dao" abstract="true"
		class="eu.europeana.corelib.db.dao.impl.DaoImpl">
	</bean>

	<bean id="corelib_db_userDao" parent="corelib_db_dao">
		<constructor-arg type="java.lang.Class"	value="eu.europeana.corelib.db.entity.UserImpl" />
	</bean>

	<bean id="corelib_db_tokenDao" parent="corelib_db_dao">
		<constructor-arg type="java.lang.Class" value="eu.europeana.corelib.db.entity.TokenImpl" />
	</bean>

</beans>