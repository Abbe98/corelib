<?xml version="1.0" encoding="UTF-8"?>

<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd
        ">
        
    <bean id="corelib_db_dao" abstract="true" class="eu.europeana.corelib.db.dao.impl.DaoImpl">
    	<!-- <property name="sessionFactory" ref="entityManagerFactory"/> -->
    </bean>
        
	<bean id="corelib_db_userDao" parent="corelib_db_dao">
		<constructor-arg type="java.lang.Class" value="eu.europeana.corelib.db.entity.User"/>
	</bean>        
        
	<bean id="corelib_db_tokenDao" parent="corelib_db_dao">
		<constructor-arg type="java.lang.Class" value="eu.europeana.corelib.db.entity.Token"/>
	</bean>        

</beans>